@page@model WebApplication1.Pages.event_dashboardModel@{    ViewData["Title"] = "Society Dashboard";}<!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <meta name="viewport" content="width=device-width, initial-scale=1.0">    <title>@ViewData["Title"]</title>    <!-- Bootstrap CSS CDN -->    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/5.1.3/css/bootstrap.min.css">    <style>        .custom-card {            max-width: 300px; /* Adjust the max-width as needed */            margin: auto; /* This centers the card in the column */            border-radius: 20px; /* Curve the border */            transition: transform 0.3s ease, background-color 0.3s ease; /* Smooth transition for hover effects */            text-decoration: none        }        .custom-card:hover {                transform: scale(1.15);                 background-color: #f8f9fa;         }        .custom-card .btn-primary {                border-radius: 50px;         }        .custom-card-body {            padding: 20px 0;            text-align: center;        }        .hide-scoiety-list {             display: none;         }    </style></head><body>    <!-- Bootstrap Navbar as a Header -->    <nav class="navbar navbar-expand-lg navbar-light bg-light mb-5  background-color: #6678B7 ">        <div class="container-fluid justify-content-center" style="background-color: #6678B7 ">            <a class="navbar-brand" href="#" style="font-size: 24px; font-weight:bold" title="Society Dashboard">Society Dashboard</a> <!-- Add title property here -->            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">                <span class="navbar-toggler-icon"></span>            </button>            </div>    </nav>    <div class="container mt-2" id="panel">        <div class="container mt-2" id="panel">            <div class="row">                <div class="col-md-6">                    <div class="card custom-card mb-4" style="background-color: #f8f0fa;">                        <div class="card-body" style="padding: 20px; text-align: center;">                            <a href="@Url.Page("approveMemberships")" class="card-title" style="font-size: 24px; font-weight: bold; cursor:pointer; text-decoration:none; color:black">Manage Members</a>                        </div>                    </div>                </div>                <div class="col-md-6">                    <div class="card custom-card mb-4" style="background-color: #f8f0fa;">                        <div class="card-body" style="padding: 20px; text-align: center;">                            <a href="@Url.Page("view_societies_head")" class="card-title" style="font-size: 24px; font-weight: bold; cursor:pointer; text-decoration:none; color:black">View Societies</a>                        </div>                    </div>                </div>                <div class="col-md-6">                    <div class="card custom-card mb-4" style="background-color: #f8f0fa;">                        <div class="card-body" style="padding: 20px; text-align: center;">                            <a href="@Url.Page("applyMembership")" class="card-title" style="font-size: 24px; font-weight: bold; cursor:pointer; text-decoration:none; color:black">Create New Society</a>                        </div>                    </div>                </div>                <div class="col-md-6">                    <div class="card custom-card mb-4" style="background-color: #f8f0fa;">                        <div class="card-body" style="padding: 20px; text-align: center;">                            <a href="@Url.Page("Addevent")" class="card-title" style="font-size: 24px; font-weight: bold; cursor: pointer; text-decoration:none; color: black">Add Event</a>                        </div>                    </div>                </div>                <div class="col-md-6" style="margin-left : 270px">                    <div class="card-body" style="text-align: center;">                        <button onclick="location.href='@Url.Page("view_feedback")'" class="btn btn-primary btn-lg" style="width: 200px">View Feedbacks</button>                    </div>                </div>            </div>        </div>    </div>    <div class="container mt-5 hide-scoiety-list "  id="society_list" >   <!-- Show list of Society  -->        <table class="table">            <tbody>                <tr data-id="1">                    <td>                      <button class="btn btn-link" onclick="handleButtonClick(1)">Row 1</button>                    </td>                </tr>                <tr data-id="2">                    <td>                        <button class="btn btn-link" onclick="handleButtonClick(1)">Row 1</button>                    </td>                </tr>                <tr data-id="3">                    <td>                        <button class="btn btn-link" onclick="handleButtonClick(1)">Row 1</button>                    </td>                </tr>            </tbody>        </table>    </div>    <div class="container mt-5 hide-scoiety-list " id="news_panel" >       <!-- Show News  -->               <table class="table">            <tbody>                <tr data-id="1">                    <td>                        <button class="btn btn-link" onclick="handleButtonClick(1)">Row 1</button>                    </td>                </tr>                <tr data-id="2">                    <td>                        <button class="btn btn-link" onclick="handleButtonClick(1)">Row 1</button>                    </td>                </tr>                <tr data-id="3">                    <td>                        <button class="btn btn-link" onclick="handleButtonClick(1)">Row 1</button>                    </td>                </tr>            </tbody>        </table>    </div>    <!-- Bootstrap Bundle with Popper -->    <script src="https://stackpath.bootstrapcdn.com/bootstrap/5.1.3/js/bootstrap.bundle.min.js"></script></body></html>